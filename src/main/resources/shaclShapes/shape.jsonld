{
 "@context":
	{
    "wasa":"http://vocab.sti2.at/wasa/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "http://schema.org/",
    "sh": "http://www.w3.org/ns/shacl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
    },

  "@graph":[
  {
	  "@id":"schema:actionShape,
	  "@type":"sh:NodeShape",
	  "sh:targetClass"{
		  "@id":"schema:Event"
	  },
	  "sh:property": [
        {
          "sh:path": {
            "@id": "schema:name"
          },
          "sh:datatype": {
            "@id": "xsd:string"
          }
        },
        {
          "sh:path": {
            "@id": "schema:startDate"
          },
          "sh:datatype": {
            "@id": "schema:Date"
          }
        },
		{
          "sh:path": {
            "@id": "schema:endtDate"
          },
          "sh:datatype": {
            "@id": "schema:Date"
          }
        }
  }
  ]
 ]

}